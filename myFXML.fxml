<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="containingHBox" alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mines.MineController">
   <children>
      <VBox fx:id="VBoxcontainer" alignment="CENTER" prefHeight="400.0" prefWidth="114.0">
         <children>
            <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="29.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label text="width" />
                  <TextField fx:id="widthText" alignment="CENTER" onAction="#getWidth" prefHeight="26.0" prefWidth="38.0" text="10">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="29.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label text="height" />
                  <TextField fx:id="heightText" alignment="CENTER" onAction="#getHeight" prefHeight="26.0" prefWidth="38.0" text="10" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="29.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label text="mines" />
                  <TextField fx:id="minesText" alignment="CENTER" onAction="#getMines" prefHeight="26.0" prefWidth="38.0" text="10" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
            <Button fx:id="restBtn" alignment="CENTER" mnemonicParsing="false" onAction="#restBtn" prefHeight="26.0" prefWidth="102.0" text="Reset">
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
